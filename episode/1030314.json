                    "slotNumber": 1,
                    "FacialExpressionMasterId": 70,
                    "BodyMotionMasterId": 14,
                    "LipSyncMasterId": 1,
                    "SpineId": 30401,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "304"
        },
        {
            "Id": 1030314037,
            "EpisodeMasterId": 1030314,
            "Order": 1,
            "GroupOrder": 37,
            "SpeakerName": "リリヤ",
            "FontSize": 2,
            "Phrase": "それに、ラモも好き。/nこんな風に会いに来てくれる。",
            "VoiceFileName": "10303140300",
