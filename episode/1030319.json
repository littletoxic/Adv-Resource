        {
            "Id": 1030319024,
            "EpisodeMasterId": 1030319,
            "Order": 1,
            "GroupOrder": 24,
            "SpeakerName": "容",
            "FontSize": 2,
            "Phrase": "いや〜、すごいもんを観た後は背筋が伸びるね〜。/n想像の上も上だったよ。",
            "BgmFileName": "403",
            "VoiceFileName": "10303190170",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 201,
                    "HeadMotionMasterId": 2,
                    "BodyMotionMasterId": 3,
                    "LipSyncMasterId": 1,
                    "SpineId": 20401,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
