            ],
            "SpeakerIconId": "204"
        },
        {
            "Id": 1030319025,
            "EpisodeMasterId": 1030319,
            "Order": 1,
            "GroupOrder": 25,
            "SpeakerName": "仁花子",
            "FontSize": 2,
            "Phrase": "あらかじめ『マクベス』を読み直してきましたけど、/n分かってても目が離せないお芝居でした……。",
            "VoiceFileName": "10303190180",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 105,
                    "HeadMotionMasterId": 1,
                    "BodyMotionMasterId": 4,
                    "LipSyncMasterId": 1,
                    "SpineId": 20301,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                },
                {
                    "slotNumber": 2,
                    "SpineId": 20401,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 4,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "203"
        },
        {
            "Id": 1030319026,
            "EpisodeMasterId": 1030319,
            "Order": 1,
            "GroupOrder": 26,
            "SpeakerName": "容",
            "FontSize": 2,
            "Phrase": "だね。目を背けたいほど悲惨なのに、/n気づいたときには、舞台に引き込まれてた。",
            "VoiceFileName": "10303190190",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 2,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
