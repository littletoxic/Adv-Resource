            "SpeakerIconId": "204"
        },
        {
            "Id": 1030319027,
            "EpisodeMasterId": 1030319,
            "Order": 1,
            "GroupOrder": 27,
            "SpeakerName": "大黒",
            "FontSize": 2,
            "Phrase": "我が主は……いかがでしたか？",
            "VoiceFileName": "10303190200",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 400,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 4,
                    "SpineId": 20201,
                    "CharacterAppearanceType": 0,
