            "SpeakerIconId": "205"
        },
        {
            "Id": 1030319034,
            "EpisodeMasterId": 1030319,
            "Order": 1,
            "GroupOrder": 33,
            "SpeakerName": "大黒",
            "FontSize": 2,
            "Phrase": "恥ずかしがることないわ……子どもは泣くのが仕事でしょ？",
            "VoiceFileName": "10303190260",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 109,
                    "BodyMotionMasterId": 6,
                    "LipSyncMasterId": 1,
                    "SpineId": 20201,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
