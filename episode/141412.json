            "EpisodeMasterId": 141412,
            "Order": 2,
            "GroupOrder": 19,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "まあ、わかってんならいっか！　",
            "VoiceFileName": "1414120191",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 202,
                    "BodyMotionMasterId": 17,
                    "LipSyncMasterId": 2,
