        {
            "Id": 141412012,
            "EpisodeMasterId": 141412,
            "Order": 1,
            "GroupOrder": 11,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "新しくできた100均……そうそう、駅ビルにできたの！　/nあそこ品揃え鬼気合入ってっからマジでおススメ！",
            "VoiceFileName": "1414120110",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 202,
                    "BodyMotionMasterId": 17,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
