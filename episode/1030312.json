            "EpisodeMasterId": 1030312,
            "Order": 1,
            "GroupOrder": 10,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "そして——",
            "VoiceFileName": "10303120100",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
