        {
            "Id": 2004801003,
            "EpisodeMasterId": 2004801,
            "Order": 1,
            "GroupOrder": 3,
            "SpeakerName": "クレア",
            "FontSize": 2,
            "Phrase": "お疲れ様です、ラモーナ先生。/n今日も盛況でしたね。",
            "VoiceFileName": "20048010030",
            "CharacterMotions": [],
            "SpeakerIconId": "1"
