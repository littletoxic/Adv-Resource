            "EpisodeMasterId": 141591,
            "Order": 1,
            "GroupOrder": 26,
            "SpeakerName": "暦",
            "FontSize": 2,
            "Phrase": "…………。",
            "VoiceFileName": "1415910240",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 107,
                    "BodyMotionMasterId": 13,
                    "SpineId": 30103,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
