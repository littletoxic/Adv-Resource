            "SpeakerIconId": "403"
        },
        {
            "Id": 1040310016,
            "EpisodeMasterId": 1040310,
            "Order": 1,
            "GroupOrder": 15,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "左右のバランス……OK！　/nはみ出た奴らを整えて……うっし、いー感じっ！　",
            "VoiceFileName": "10403100150",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 8,
                    "LipSyncMasterId": 1,
