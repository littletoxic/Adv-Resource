        },
        {
            "Id": 1040310007,
            "EpisodeMasterId": 1040310,
            "Order": 1,
            "GroupOrder": 7,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "厚塗りにならんように……使うコンシーラーはオレンジ系。/nオレンジいいよね～。なんかアガるし！　",
            "VoiceFileName": "10403100070",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 103,
                    "BodyMotionMasterId": 11,
                    "LipSyncMasterId": 2,
                    "SpineId": 0,
