                }
            ],
            "SpeakerIconId": "403"
        },
        {
            "Id": 1040310008,
            "EpisodeMasterId": 1040310,
            "Order": 1,
            "GroupOrder": 8,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "けど、こんなん出てることにも気づいてなかったんだから笑っちゃうわ。/n――はい、クマさんは森へお帰り。バ～イバイ！　",
            "VoiceFileName": "10403100080",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 201,
                    "BodyMotionMasterId": 20,
                    "LipSyncMasterId": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
