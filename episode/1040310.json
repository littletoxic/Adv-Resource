            "SpeakerIconId": "403"
        },
        {
            "Id": 1040310020,
            "EpisodeMasterId": 1040310,
            "Order": 1,
            "GroupOrder": 19,
            "SpeakerName": "カミラ",
            "FontSize": 2,
            "Phrase": "で、リップ……。/n……グロスでツヤ感出して……。",
            "VoiceFileName": "10403100190",
            "CharacterMotions": [
