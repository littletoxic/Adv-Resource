            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "SpineId": 0,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
                    "CharacterLayerType": 1,
                    "SpineSize": 2
                }
            ],
            "SpeakerIconId": "1"
        },
        {
            "Id": 2005703034,
            "EpisodeMasterId": 2005703,
            "Order": 1,
            "GroupOrder": 34,
            "SpeakerName": "初魅",
            "FontSize": 2,
            "Phrase": "テトラさんという創設者の方に助言をもらっているが、/n概ね問題なくやれている。心配無用だ。",
            "VoiceFileName": "20057030310",
            "CharacterMotions": [
