        },
        {
            "Id": 2003606061,
            "EpisodeMasterId": 2003606,
            "Order": 1,
            "GroupOrder": 58,
            "SpeakerName": "いろは",
            "FontSize": 2,
            "Phrase": "……。",
            "VoiceFileName": "20036060530",
            "CharacterMotions": [
                {
                    "slotNumber": 1,
                    "FacialExpressionMasterId": 400,
                    "BodyMotionMasterId": 13,
                    "SpineId": 40801,
                    "CharacterAppearanceType": 0,
                    "CharacterPosition": 3,
